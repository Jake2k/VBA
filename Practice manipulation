Sub CreateChoice(choice As Integer, savechoice As Integer)
ReRoll:
        Randomize
        choice = Int((4 * Rnd) + 1)
        
        If choice = 1 And savechoice = 3 Then
            GoTo ReRoll
        Else
        If choice = 2 And savechoice = 4 Then
            GoTo ReRoll
        Else
        If choice = 3 And savechoice = 1 Then
            GoTo ReRoll
        Else
        If choice = 4 And savechoice = 2 Then
            GoTo ReRoll
        Else
        End If
        End If
        End If
        End If
End Sub
Sub Movement(prmChoice As Integer, prmRow As Integer, prmCol As Integer, prmCount As Integer)
    Debug.Print "ROW" & prmRow
    Debug.Print "COL" & prmCol
    Debug.Print "Choice" & prmChoice
    Debug.Print "Count" & prmCount
    Debug.Print ""
    If prmChoice = 1 Then 'UP
        prmRow = prmRow - 1
        If shtData.Cells(prmRow, prmCol).Value = "" Then
            shtData.Cells(prmRow, prmCol).Value = prmCount
            Else
        
            End If
        Else
    If prmChoice = 2 Then 'Right
        prmCol = prmCol + 1
        If shtData.Cells(prmRow, prmCol).Value = "" Then
            shtData.Cells(prmRow, prmCol).Value = prmCount
            Else
            
            End If
        Else
    If prmChoice = 3 Then 'Down
        prmRow = prmRow + 1
        If shtData.Cells(prmRow, prmCol).Value = "" Then
            shtData.Cells(prmRow, prmCol).Value = prmCount
            Else
           
            End If
        Else
    If prmChoice = 4 Then 'Left
        prmCol = prmCol - 1
        If shtData.Cells(prmRow, prmCol).Value = "" Then
            shtData.Cells(prmRow, prmCol).Value = prmCount
            Else
            
            End If
    End If
    End If
    End If
    End If
    
End Sub


Sub wave()
    Dim row As Integer
    Dim col As Integer
    
    row = 1
    col = 1
    
    While col < 100
        ColourSq row, col
        col = col + 1
        row = 1
    Wend
    
End Sub

Sub ColourSq(prmRow As Integer, prmCol As Integer)
    Dim rowHeight As Integer
    Dim i As Integer

    rowHeight = prmCol
    
    For i = 1 To rowHeight
        shtData.Cells(prmRow, prmCol).Value = prmCol
        prmRow = prmRow + 1
        Next i
        
    
End Sub


Sub Oscillation()
  Dim row As Integer
    Dim col As Integer
    
    row = 1
    col = 1
    
    While col < 100
        ColourSq2 row, col
        col = col + 1
        row = 1
    Wend
    
End Sub

Sub ColourSq2(prmRow As Integer, prmCol As Integer)
    Dim rowHeight As Integer
    Dim i As Integer

    rowHeight = Int((20 * Rnd) + 1)
    
    For i = 1 To rowHeight
        shtData.Cells(prmRow, prmCol).Value = prmCol
        prmRow = prmRow + 1
        Next i
End Sub


Sub dotToDot()
    Dim row As Integer
    Dim col As Integer
    Dim row2 As Integer
    Dim col2 As Integer
    
    Randomize
    row = Int((99 * Rnd) + 1)
    col = Int((99 * Rnd) + 1)
  
    shtData.Cells(row, col).Value = 1
    
ReRoll:
    row2 = Int((99 * Rnd) + 1)
    col2 = Int((99 * Rnd) + 1)
   
    If row2 = row Then
    GoTo ReRoll
    Else
    End If

    If col2 = col Then
    GoTo ReRoll
    Else
    End If

    shtData.Cells(row2, col2).Value = 100
    CalculateMovement row, col, row2, col2
    
End Sub

Sub CalculateMovement(prmRow As Integer, prmCol As Integer, prmRow2 As Integer, prmCol2 As Integer)
    Dim rowMovement As Integer
    Dim colMovement As Integer
    Dim startPointRow As Integer
    Dim startPointCol As Integer
    Dim minusFlag As Integer
    Dim startPointRowReverse As Integer
    Dim flag As Integer
    Dim flag2 As Integer
    
    If prmCol > prmCol2 And prmRow < prmRow2 Then
        startPointRow = prmRow2
        startPointCol = prmCol2
        rowMovement = prmRow2 - prmRow - 1
        colMovement = prmCol - prmCol2
        Debug.Print "TEST TEST TEST"
        
        startPointRowReverse = prmRow
        flag = 1
        flag2 = 1
        
    Else
    End If
    
    
    If prmRow > prmRow2 Then
        rowMovement = prmRow - prmRow2 'UP
        startPointRow = prmRow2
    Else
        rowMovement = prmRow2 - prmRow 'DOWN
        startPointRow = prmRow
    End If
    
    If prmCol > prmCol2 Then
        colMovement = prmCol - prmCol2 'RIGHT
        startPointCol = prmCol2
    Else
        colMovement = prmCol2 - prmCol - 1 'RIGHT
        startPointCol = prmCol
    End If
    
    Debug.Print "RowMovement:" & rowMovement & " startPointRow:" & startPointRow
    Debug.Print "ColMovement:" & colMovement & " startPointCol:" & startPointCol
    
    Drawline rowMovement, colMovement, startPointCol, startPointRow, startPointRowReverse, flag, flag2
    
End Sub

Sub Drawline(prmRowMovement As Integer, prmColMovement As Integer, prmStartPointCol As Integer, prmStartPointRow As Integer, prmStartPointRowReverse As Integer, prmFlag As Integer, prmFlag2 As Integer)
    Dim i As Integer
    
    For i = 1 To prmRowMovement
        If prmFlag2 = 1 Then
        prmStartPointRow = prmStartPointRow - 1
        prmFlag2 = 0
        Else
        End If
        
        prmStartPointRow = prmStartPointRow + 1
        
        shtData.Cells(prmStartPointRow, prmStartPointCol).Value = 69
    Next i
    
   
    
    For i = 0 To prmColMovement - 1
        If prmFlag = 1 Then
            prmStartPointRow = prmStartPointRowReverse
            prmStartPointCol = prmStartPointCol - 1
            prmFlag = 0
        Else
        End If
        
        prmStartPointCol = prmStartPointCol + 1
        shtData.Cells(prmStartPointRow, prmStartPointCol).Value = 69
            
    Next i
    
  
    
End Sub

Private Function CalculateValues(prmOperator As String, prmVal1 As Double, prmVal2 As Double) As Double
    
Select Case prmOperator
    Case "x"
        CalculateValues = (prmVal1 * prmVal2)
    Case "+"
        CalculateValues = (prmVal1 + prmVal2)
    Case "-"
        CalculateValues = (prmVal1 - prmVal2)
    Case Else
        CalculateValues = 0
End Select
    
End Function

Sub CardShuffler()
    Dim deck As Integer
    Dim row As Integer
    Dim col As Integer
    
    deck = 52
    
    
End Sub


