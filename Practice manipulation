Sub test()
    Dim col As Integer
    Dim row As Integer
    Dim i As Integer
    row = 4
    col = 1
    
    While row < 100
        Randomize
        shtData.Cells(row, col).Value = Int((60 * Rnd) + 1)
        col = col + 1
            If col = 100 Then
                row = row + 1
                col = 1
            End If
    Wend
           
End Sub

Sub randomSq()
    Dim rrow As Integer
    Dim rcol As Integer
    Dim count As Integer
    
    While count < 500
        Randomize
        shtData.Cells(Int((100 * Rnd) + 1), Int((99 * Rnd) + 1)).Value = Int((60 * Rnd) + 1)
        count = count + 1
    Wend
End Sub

Sub pathfinder()
    Dim row As Integer
    Dim col As Integer
    Dim count As Integer
    Dim choice As Integer
    Dim savechoice As Integer
    
    
    count = 1
        While count < 50

        If count = 1 Then
'            Randomize
'            row = Int((100 * Rnd) + 1)
'            col = Int((100 * Rnd) + 1)
            row = 49
            col = 50
            shtData.Cells(row, col).Value = count
            count = count + 1
        Else
            CreateChoice choice, savechoice

    
        If choice = 1 Then
            Movement choice, row, col, count 'UP
            count = count + 1
            savechoice = 1
            Else
        If choice = 2 Then
            Movement choice, row, col, count 'Right
            count = count + 1
            savechoice = 2
            Else
        If choice = 3 Then
            Movement choice, row, col, count 'Down
            count = count + 1
            savechoice = 3
            Else
        If choice = 4 Then
            Movement choice, row, col, count 'Left
            count = count + 1
            savechoice = 4
            Else
        End If
        End If
        End If
        End If
                                                     
        End If
    Wend
End Sub

