Sub randomHex()
        Dim hex As String
        Dim hash As String
        Dim hashGen As Long
        Dim i As Integer
        Dim part1 As String
        Dim part2 As String
        Dim part3 As String
        Dim part4 As String
        Dim part5 As String
        Dim part6 As String
        
        
        Randomize
        hash = "hgujhgtf"
        
        For i = 1 To Len(hash)
           hashGen = Asc(Mid(hash, i, 1)) * i + hashGen
           Next i
        
        Debug.Print "hashgen: " & hashGen
        
        part1 = Int((14 * Rnd) + 1)
        part2 = Int((14 * Rnd) + 1)
        part3 = Int((14 * Rnd) + 1)
        part4 = Int((14 * Rnd) + 1)
